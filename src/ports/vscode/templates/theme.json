        "entity.name.section.markdown",
        "storage.type.annotation.dart"
      ],
      "settings": {
        "foreground": "{{ accent3 }}"
      }
    },
    {
      "name": "Accent5",
      "scope": [
        "invalid",
        "markup.deleted"
      ],
      "settings": {
        "foreground": "{{ accent5 }}"
      }
    },
